{
  "name": "cc-inspector",
  "version": "0.0.1",
  "description": "The package template for getting started.",
  "author": "Cocos Creator",
  "main": "main.js",
  "main-menu": {
    "cc-inspector/打开": {
      "message": "cc-inspector:open",
      "icon":"icon/cc-inspector.png"
    },
    "cc-inspector/帮助":{
      "message": "cc-inspector:help",
      "icon":"icon/help.png"
    }
  },
  "panel": {
    "main": "panel/index.js",
    "type": "dockable",
    "title": "节点列表",
    "width": 400,
    "height": 300
  },
  "panel.preview-web": {
    "main": "panel-preview-web/index.js",
    "type": "dockable",
    "title": "web预览",
    "width": 400,
    "height": 300
  },
  "panel.help":{
    "main": "panel-help/index.js",
    "type": "fixed-size",
    "title": "帮助",
    "width": 275,
    "height": 470
  },
  "panel.info":{
    "main": "panel-info/index.js",
    "type": "dockable",
    "title": "节点信息",
    "width": 400,
    "height": 300
  },
  "runtime-resource": {
    "path": "plugin",
    "name": "plugin"
  },
  "reload": {
    "ignore": [
      "plugin/**/*"
    ]
  },
  "dependencies": {
    "ws": "^6.0.0"
  }
}
